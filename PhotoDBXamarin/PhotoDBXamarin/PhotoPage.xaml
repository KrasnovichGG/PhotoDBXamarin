<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PhotoDBXamarin.PhotoPage"
             xmlns:local ="clr-namespace:PhotoDBXamarin">
    <StackLayout Margin="40,30" Spacing="20">
        <Label Text="{x:Static local:PhotoPage.title}" Padding="25" BackgroundColor="LightGray"
            />
        <Image x:Name="Gay" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"
               Margin="0,0,0,100"/>
    </StackLayout>
</ContentPage>